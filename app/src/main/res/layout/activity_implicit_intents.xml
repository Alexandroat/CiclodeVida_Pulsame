<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="es.tessier.carlos.misproyectos.ImplicitIntents">

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="www.uem.es"
        android:id="@+id/btnPage"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="onClickWebBrowser" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Teléfono UEM"
        android:id="@+id/btnTel"
        android:layout_below="@+id/btnPage"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="onClickCall" />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Localización UEM"
        android:id="@+id/btnLoc"
        android:layout_below="@+id/btnTel"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:onClick="onClickShowMap" />
</RelativeLayout>
